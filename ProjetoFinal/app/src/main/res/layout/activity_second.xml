<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mmrosa.projetofinal.SecondActivity">

    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/tvNome"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/ctNome"
            android:layout_marginTop="11dp"
            android:text="Nome"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <EditText
            android:id="@+id/ctNome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/tvNome"
            android:layout_marginStart="11dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="41dp" />

        <TextView
            android:id="@+id/tvCpf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/ctNome"
            android:layout_below="@+id/ctNome"
            android:layout_marginTop="11dp"
            android:text="CPF"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="100dp" />

        <EditText
            android:id="@+id/ctCpf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tvCpf"
            android:layout_below="@+id/tvCpf"
            android:layout_marginTop="13dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="129dp" />

        <TextView
            android:id="@+id/tvIdade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/ctCpf"
            android:layout_below="@+id/ctCpf"
            android:layout_marginTop="11dp"
            android:text="Idade"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="187dp" />

        <EditText
            android:id="@+id/ctIdade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tvIdade"
            android:layout_below="@+id/tvIdade"
            android:layout_marginTop="11dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="220dp" />

        <TextView
            android:id="@+id/tvTelefone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/ctIdade"
            android:layout_below="@+id/ctIdade"
            android:layout_marginTop="11dp"
            android:text="Telefone"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="278dp" />

        <EditText
            android:id="@+id/ctTel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tvTelefone"
            android:layout_below="@+id/tvTelefone"
            android:layout_marginTop="11dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="306dp" />

        <TextView
            android:id="@+id/tvEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/ctTel"
            android:layout_below="@+id/ctTel"
            android:layout_marginTop="12dp"
            android:text="Email"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="361dp" />

        <EditText
            android:id="@+id/ctEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tvEmail"
            android:layout_below="@+id/tvEmail"
            android:layout_marginTop="13dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="389dp" />

        <Button
            android:id="@+id/btnVoltar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/ctEmail"
            android:layout_below="@+id/ctEmail"
            android:layout_marginTop="11dp"
            android:text="VOLTAR"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="446dp" />

        <Button
            android:id="@+id/btnInserir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/btnVoltar"
            android:layout_alignBottom="@+id/btnVoltar"
            android:layout_alignEnd="@+id/ctEmail"
            android:layout_marginEnd="11dp"
            android:text="INSERIR"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="446dp" />

        <Button
            android:id="@+id/btnListar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/btnInserir"
            android:layout_alignBottom="@+id/btnInserir"
            android:layout_marginStart="18dp"
            android:layout_toEndOf="@+id/ctEmail"
            android:text="LISTAR"
            tools:layout_editor_absoluteX="243dp"
            tools:layout_editor_absoluteY="446dp" />


    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
